<h1>Editing tag_favorite</h1>

<%= error_messages_for :tag_favorite %>

<% form_for(@tag_favorite) do |f| %>
  <p>
    <b>Tag</b><br />
    <%= f.text_field :tag_id %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @tag_favorite %> |
<%= link_to 'Back', tag_favorites_path %>
