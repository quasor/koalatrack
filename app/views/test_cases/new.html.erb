<%= javascript_include_tag "fckeditor/fckeditor" %>
<h1>New test case</h1>

<%= error_messages_for :test_case %>

<% form_for(@test_case, :html => { :multipart => true }) do |f| %>
  <%= render :partial => 'form', :object => f%>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', test_cases_path %>
